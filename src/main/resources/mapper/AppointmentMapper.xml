<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsx.ai.english.mapper.WordMapper">
    <delete id="removeWordById">
        delete from word where id = #{id}
    </delete>

    <select id="selectWordId" resultType="java.lang.Integer">
        select id from word where english = #{english}
    </select>
</mapper>